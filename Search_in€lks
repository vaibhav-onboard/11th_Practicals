nums = input("Enter a list of numbers separated by space: ").split()

search_element = input("Enter the element to search: ")

found = False

for num in nums:

    if num == search_element:

        found = True

        break

if found:

    print(f"Element {search_element} found in the list")

else:

    print(f"Element {search_element} not found in the list")

